{
  "author": "Dennis Weiersh√§user",
  "name": "sia",
  "version": "3.1.0",
  "scripts": {
    "clean": "rm -rf ./static",
    "build": "clear; npm run clean; webpack --env production --progress",
    "build:watch": "nodemon -x \"clear; npm run clean; webpack --env production\"",
    "build:server": "npm run clean; npm install --prefix ../ -d && npm install -d && webpack --env production --progress",
    "install:all": "npm install --prefix ../ -d && npm install",
    "dev": "clear; webpack --env development --watch"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "nodemon": "^1.18.4",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@cartok/hierarchy-select": "^1.1.15",
    "@cartok/object-assign-deep": "^0.4.2",
    "dbly-linked-list": "^0.2.0",
    "l3p-core": "^3.0.13",
    "lodash-es": "^4.17.10"
  },
  "nodemonConfig": {
    "watch": [
      "test/**/*",
      "src/**/*"
    ],
    "ext": "js",
    "delay": 500,
    "verbose": true
  }
}
